
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous"> 

<%# if logged in then redirect to projects page else show home page %>
<div class = "jumbotron "align= "center">
  <h1>Home page</h1>
  <hr/>
  <h2>Demo project</h2>
  <hr/>
<% if user_signed_in? %> 
  <p>
    <%= link_to "View all Projects", projects_path,class: "btn btn-primary btn-lg btn-xlarge" %>
  </p>
<% else %>

  <p>
    <%= link_to "Sign in for the app", new_user_session_path,class: "btn btn-warning btn-lg btn-xlarge" %>
    <%= link_to "Sign up for the app", new_user_registration_path,class: "btn btn-danger btn-lg btn-xlarge" %>
    <%#= link_to "View all Projects", projects_path,class: "btn btn-danger btn-lg btn-xlarge" %>
  </p>
</div>
<%end%>